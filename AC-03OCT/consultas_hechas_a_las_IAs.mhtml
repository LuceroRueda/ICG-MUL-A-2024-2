    o =3D sorted_points[i]
    a =3D sorted_points[(i + 1) % n]
    b =3D sorted_points[(i + 2) % n]
    cp =3D cross_product(o, a, b)
    cross_products.append(cp)

# Analizar los signos de los productos cruzados
positive =3D all(cp &gt; 0 for cp in cross_products if cp !=3D 0)
negative =3D all(cp &lt; 0 for cp in cross_products if cp !=3D 0)

# Resultado final
if positive or negative:
    result =3D "Los puntos forman una figura convexa."
else:
    result =3D "Los puntos no forman una figura convexa."

