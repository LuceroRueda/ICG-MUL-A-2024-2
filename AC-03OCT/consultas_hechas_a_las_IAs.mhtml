function dibujarLinea(x1, y1, x2, y2) {
    const linea =3D crearElemento('line', {
        x1: x1,
        y1: y1,
        x2: x2,
        y2: y2,
        stroke: 'black',
        'stroke-width': 1
    });
    return linea;
}

// Dibujar una circunferencia
function dibujarCircunferencia(cx, cy, r) {
    const circunferencia =3D crearElemento('circle', {
        cx: cx,
        cy: cy,
        r: r,
        stroke: 'black',
        'stroke-width': 1,
        fill: 'none'
